<?xml version="1.0" encoding="UTF-8"?>
<BusinessProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case progress process</fullName>
    <description>process to track progress of case through status field</description>
    <isActive>true</isActive>
    <values>
        <fullName>Approved</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Awaiting Approval</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Closed</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>New</fullName>
        <default>true</default>
    </values>
    <values>
        <fullName>On Hold</fullName>
        <default>false</default>
    </values>
</BusinessProcess>
